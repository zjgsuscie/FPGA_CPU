library verilog;
use verilog.vl_types.all;
entity pc is
    port(
        clk             : in     vl_logic;
        rst             : in     vl_logic;
        load            : in     vl_logic;
        inc             : in     vl_logic;
        din             : in     vl_logic_vector(5 downto 0);
        dout            : out    vl_logic_vector(5 downto 0)
    );
end pc;
